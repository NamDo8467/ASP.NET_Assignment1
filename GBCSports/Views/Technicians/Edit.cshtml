@model Technicians

<form method="post" autocomplete="off">
    <h2 class="text-primary">Edit Technician</h2>
    <div class="mb-3">
        <label asp-for="FirstName" class="form-label"></label>
        <span class="text-danger" asp-validation-for="FirstName"></span>
        <input asp-for="FirstName" type="text" class="form-control">
    </div>
    <div class="mb-3">
        <label asp-for="LastName" class="form-label"></label>
        <span class="text-danger" asp-validation-for="LastName"></span>
        <input asp-for="LastName" type="text" class="form-control">
    </div>
    <div class="mb-3">
        <label asp-for="Email" class="form-check-label"></label>
        <input asp-for="Email" type="text" class="form-control">
    </div>
    <div class="mb-3">
        <label asp-for="Phone" class="form-check-label"></label>
        <input asp-for="Phone" type="text" class="form-control">
    </div>
    <button type="submit" class="btn btn-primary">Update</button>
    <a class="btn btn-danger" asp-controller="Technicians" asp-action="Index">
        Cancel
    </a>
</form>

@section Scripts{
    @{
        <partial name="_ValidationScriptsPartial" />
    }
}